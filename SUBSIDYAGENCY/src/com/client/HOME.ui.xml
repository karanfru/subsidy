			<m:MaterialLink ui:field="ww" iconType="INFO" text="HOW TO APPLY"  />
			<m:MaterialLink iconType="HELP" text="HELP CENTRE" />
			<m:MaterialLink iconType="SEND" text="ABOUT US" />

		</m:MaterialSideNavDrawer>
		
		
		<m:MaterialContainer ui:field="c">
		
		 <m:MaterialRow  ui:field="tt1" paddingTop="60" marginLeft="350">
        <m:MaterialSlider ui:field="slider" fullscreen="false"  width="1000px" height="1000px" >
        	<m:MaterialSlideItem>
          <m:MaterialImage url="https://i.ytimg.com/vi/KHV5AgexTK8/maxresdefault.jpg"/>
          <m:MaterialSlideCaption textAlign="CENTER">
            <m:MaterialTitle title="" description=""/>
