<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m='urn:import:gwt.material.design.client.ui'
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style gss="true">
	body {
		background: #ffe082;
	}
	 .customFooter{
	  backgroundcolor:#64dd17;
      padding-left: 0 !important;
      margin-bottom: 20px;
    }
	</ui:style>
	<m:MaterialPanel>
		
		<m:MaterialHeader ui:field="header">
		
			<m:MaterialNavBar activates="sidenavDrawer"
				 backgroundColor="AMBER" shadow="1">
				<m:MaterialNavBrand paddingLeft="60"
					text="SUBSIDY AGENCY" />
			</m:MaterialNavBar>
		
			<m:MaterialTab ui:field="tabIntro" shadow="1" indicatorColor="YELLOW">
              <m:MaterialTabItem waves="YELLOW">
                <m:MaterialLink text="HOME" ui:field="cc"  textColor="WHITE" backgroundColor="DEEP_ORANGE"/>
              </m:MaterialTabItem>
              <m:MaterialTabItem waves="YELLOW">
                <m:MaterialLink ui:field="ll" text="LIST OF SUBSIDY" href="#tab2" textColor="WHITE" backgroundColor="DEEP_ORANGE"/>
              </m:MaterialTabItem>
              <m:MaterialTabItem waves="YELLOW">
                <m:MaterialLink ui:field="t1" text="USER LOGIN" href="#tab2" textColor="WHITE" backgroundColor="DEEP_ORANGE"/>
              </m:MaterialTabItem>
              <m:MaterialTabItem waves="YELLOW">
                <m:MaterialLink ui:field="t3" text="ADMIN LOGIN" href="#tab2" textColor="WHITE" backgroundColor="DEEP_ORANGE"/>
              </m:MaterialTabItem>             
              <m:MaterialTabItem waves="YELLOW">
                <m:MaterialLink ui:field="t2" text="REGISTER" href="#tab3" textColor="WHITE" backgroundColor="DEEP_ORANGE"/>
              </m:MaterialTabItem>
            </m:MaterialTab>
		</m:MaterialHeader>

		<m:MaterialSideNavDrawer m:id="sidenavDrawer"
