            <m:MaterialTitle title="" description=""/>
          </m:MaterialSlideCaption>
        </m:MaterialSlideItem>
        
        <m:MaterialSlideItem>
          <m:MaterialImage url="https://res.klook.com/images/fl_lossy.progressive,q_65/c_fill,w_1295,h_863,f_auto/w_80,x_15,y_15,g_south_west,l_klook_water/activities/oocietx3xrox715kmjq5/JaipurVillageJeepSafari.jpg"/>
          <m:MaterialSlideCaption textAlign="CENTER">
            <m:MaterialTitle title="Happy Womens" description="After getting Subsidy"/>
